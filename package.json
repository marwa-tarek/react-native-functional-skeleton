{
  "name": "reacttsproject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "watchman watch-del-all && rm -rf yarn.lock package-lock.json node_modules ios/Pods ios/Podfile.lock ios/Goldi.xcworkspace android/app/build ~/Library/Developer/Xcode/DerivedData && yarn install && cd ios && pod update && cd ..",
    "clean:ios": "watchman watch-del-all && rm -rf ios/Pods ios/Podfile.lock ios/Goldi.xcworkspace ~/Library/Developer/Xcode/DerivedData && cd ios && pod update && cd ..",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android-gems": "cd android && bundle install && cd ..",
    "ios-gems": "cd ios && bundle install && cd ..",
    "pods": "cd ios && bundle exec pod install",
    "android-bundle": "react-native bundle --reset-cache --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "ios-bundle": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "images": "node scripts/images.ts",
    "fonts": "node scripts/fonts.ts",
    "link-pkgs": "react-native link",
    "test": "jest",
    "type-check": "tsc",
    "rename": "npx react-native-rename",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write {.,**}/*.js",
    "format:json": "prettier --write {.,**}/*.json",
    "format:md": "prettier --write {.,**}/*.md",
    "format:ts": "prettier --write {.,**}/*.{ts,tsx}",
    "check-format": "npx prettier -c \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "lint": "npx eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "npx eslint . --fix",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@react-native-firebase/analytics": "^13.0.1",
    "@react-native-firebase/app": "^13.0.1",
    "@react-native-firebase/auth": "^13.0.1",
    "@react-native-firebase/firestore": "^13.0.1",
    "@react-native-firebase/functions": "^13.0.1",
    "@react-native-firebase/messaging": "^13.0.1",
    "@react-native-firebase/storage": "^13.0.1",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^5.x",
    "@reduxjs/toolkit": "^1.6.1",
    "@thecodingmachine/redux-toolkit-wrapper": "^2.0.1",
    "@types/node": "^16.11.9",
    "@types/redux-logger": "^3.0.9",
    "axios": "^0.21.4",
    "eslint-plugin-prettier": "^4.0.0",
    "flux-standard-action": "^2.1.2",
    "i18next": "^20.6.1",
    "patch-package": "^6.4.7",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-i18next": "^11.12.0",
    "react-native-device-info": "^8.4.8",
    "react-native-flipper": "^0.113.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.2.4",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.2",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-flipper": "^2.0.1",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@babel/eslint-parser": "^7.16.3",
    "@types/jest": "^27.0.2",
    "@types/react-native": "^0.66.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/fbemitter": "^2.0.32",
    "@types/react": "^17.0.34",
    "@types/react-redux": "^7.1.20",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "^5.8.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-test-renderer": "^17.0.2",
    "react-native-codegen": "^0.0.7",
    "redux-logger": "^3.0.6",
    "typescript": "^4.4.4",
    "jest": "^26.6.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.0"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
